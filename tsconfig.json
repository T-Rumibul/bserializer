{
  "extends":"./node22.json",
  "compilerOptions": {
    "removeComments": false,
    "declaration": true,
    "baseUrl": "./",
    "paths": {
      "*": ["./src/*"]
    },
    "outDir": "./dist",
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "forceConsistentCasingInFileNames": true,
    "rewriteRelativeImportExtensions": true
  },
  "include": ["src"],
  "exclude": ["node_modules", "dist"],
}
